@article{lubyIncreasingSuicideRates2019,
  title = {Increasing {{Suicide Rates}} in {{Early Adolescent Girls}} in the {{United States}} and the {{Equalization}} of {{Sex Disparity}} in {{Suicide}}: {{The Need}} to {{Investigate}} the {{Role}} of {{Social Media}}},
