@article{narensScalesMeasurement1981,
  title = {On the Scales of Measurement},
  author = {Narens, Louis},
  year = {1981},
  month = dec,
  journal = {Journal of Mathematical Psychology},
  volume = {24},
  number = {3},
  pages = {249--275},
  issn = {0022-2496},
  doi = {10.1016/0022-2496(81)90045-6},
  urldate = {2018-11-20},
  abstract = {Let X = {\textlangle}X, {$\geqq$}, R1, R2{\dots}{\textrangle} be a relational structure, {\textlangle}X, {$\geqq\rangle$} be a Dedekind complete, totally ordered set, and n be a nonnegative integer. X is said to satisfy n-point homogeneity if and only if for each x1,{\dots}, xn, y1,{\dots}, yn such that x1 {$\succ$} x2 {$\succ$} {\dots} {$\succ$} xn and y1 {$\succ$} y2 {\dots} {$\succ$} yn, there exists an automorphism {$\alpha$} of X such that {$\alpha$}(x1) = yi. X is said to satisfy n-point uniqueness if and only if for all automorphisms {$\beta$} and {$\gamma$} of X, if {$\beta$} and {$\gamma$} agree at n distinct points of X, then {$\beta$} and {$\gamma$} are identical. It is shown that if X satisfies n-point homogeneity and n-point uniqueness, then n {$\leqq$} 2, and for the case n = 1, X is ratio scalable, and for the case n = 2, interval scalable. This result is very general and may in part provide an explanation of why so few scale types have arisen in science. The cases of 0-point homogeneity and infinite point homogeneity are also discussed.}
}

@article{naritaCausalInferenceObservational2023,
  title = {Causal Inference with Observational Data: {{A}} Tutorial on Propensity Score Analysis},
  shorttitle = {Causal Inference with Observational Data},
  author = {Narita, Kaori and Tena, J. D. and Detotto, Claudio},
  year = {2023},
  month = feb,
  journal = {The Leadership Quarterly},
  pages = {101678},
  issn = {1048-9843},
  doi = {10.1016/j.leaqua.2023.101678},
  urldate = {2023-03-02},
  abstract = {When treatment cannot be manipulated, propensity score analysis provides a useful way to making causal claims under the assumption of no unobserved confounders. However, it is still rarely utilised in leadership and applied psychology research. The purpose of this paper is threefold. First, it explains and discusses the application and key assumptions of the method with a particular focus on propensity score weighting. This approach is readily implementable since a weighted regression is available in most statistical software. Moreover, the approach can offer a ``double robust'' protection against misspecification of either the propensity score or the outcome model by including confounding variables in both models. A second aim is to discuss how propensity score analysis (and propensity score weighting, specifically) has been conducted in recent management studies and examine future challenges. Finally, we present an advanced application of the approach to illustrate how it can be employed to estimate the causal impact of leadership succession on performance using data from Italian football. The case also exemplifies how to extend the standard single treatment analysis to estimate the separate impact of different managerial characteristic changes between the old and the new manager.},
  langid = {english}
}

